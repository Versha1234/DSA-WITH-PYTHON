list = []
num=int(input("enter the size of array:"))
for i in range(1,num+1):
    element=int(input("enter the no:"))
    list.append(element)
print(list)

sumeven=0
sumodd=0
    
for x in list:
        
    if x%2==0:
        sumeven=sumeven + x
        
    else:
        sumodd=sumodd + x
        
print('sum of even no is:',sumeven)
print('sum of odd no is:',sumodd)
    
