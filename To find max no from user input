list = []
num=int(input("enter the size of array:"))
for i in range(1,num+1):
    element=int(input("enter the no:"))
    list.append(element)
print(list)

def max(list):
    max = list[0]
    for x in list:
        
        if x > max:
            max=x
    return max
            
print(max(list))
